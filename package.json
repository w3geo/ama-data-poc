{
  "name": "ama-highlight-poc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "create-dirs": "mkdirp dist",
    "postinstall": "npm run create-dirs",
    "external-data:convert": "cross-unzip data/VGD-Oesterreich_gst.zip data/ && shp2json --encoding utf-8 data/_Oesterreich_BEV_VGD_LAM.shp > data/VGD_Oesterreich_gst.geojson",
    "external-data:prepare": "node scripts/gem-bez-bl.js",
    "data:pct": "node --max-old-space-size=8192 scripts/prepare-data --topic=pct",
    "data:t1v": "node --max-old-space-size=8192 scripts/prepare-data --topic=t1v",
    "data": "npm run external-data:convert && npm run external-data:prepare && npm run tiles",
    "tiles-base": "tippecanoe --no-tiny-polygon-reduction --low-detail=10 --simplification=10 --detect-shared-borders --no-tile-compression --maximum-tile-bytes=1028000 data/bl.json data/bez.json data/gem.json data/kg.json",
    "tiles:pct": "npm run data:pct && rimraf dist/tiles-pct && npm run tiles-base -- --output-to-directory dist/tiles-pct",
    "tiles:t1v": "npm run data:t1v && rimraf dist/tiles-t1v && npm run tiles-base -- --output-to-directory dist/tiles-t1v",
    "tiles": "npm run tiles:pct && npm run tiles:t1v",
    "start": "webpack-dev-server",
    "build": "webpack",
    "publish": "node scripts/gh-pages.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mapbox/mapbox-gl-style-spec": "^13.7.2",
    "ol": "^6.1.1-dev.1573743852980",
    "ol-mapbox-style": "^5.0.2"
  },
  "devDependencies": {
    "cross-zip-cli": "^1.0.0",
    "css-loader": "^3.1.0",
    "geojson-dissolve": "^3.1.0",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "node-wget": "^0.4.3",
    "reproject": "^1.2.5",
    "rimraf": "^3.0.0",
    "shapefile": "^0.6.6",
    "simple-arg-parser": "^1.2.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
